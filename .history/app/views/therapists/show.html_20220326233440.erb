<h1><%= @therapist.name %></h1>
<ul class="list-group">

  <% @appointments.each do |day, items| %>

  <li class="list-group-item"><%= day %></li>
    
    <% items.each do |a|%>

    
        <li class="list-group-item"><%= a.start.strftime("%H:%M") %> + <%= a.duration %> minutes</li>
    </ul>

    <% end %>

  </li>

<% end %>

</ul>
